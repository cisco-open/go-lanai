{
  "api_access": {
    "/apim": {
      "/api/v1/scopes/list": {
        "GET": [
          "VIEW_APIKEY"
        ]
      },
      "/api/v1/keys": {
        "GET": [
          "VIEW_APIKEY"
        ]
      },
      "/api/v1/keys/generate": {
        "POST": [
          "MANAGE_APIKEY"
        ]
      },
      "/api/v1/keys/*": {
        "DELETE": [
          "MANAGE_APIKEY"
        ]
      },
      "/api/v1/enforce/scopes/lookup": {
        "GET": [
          "IS_API_ADMIN"
        ]
      },
      "/api/v1/enforce/mappings/lookup": {
        "GET": [
          "IS_API_ADMIN"
        ]
      },
      "/api/v1/enforce/keys/verify": {
        "GET": [
          "IS_API_ADMIN"
        ]
      }
    }
  }
}